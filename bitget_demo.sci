function bitget_demo()
    a = uint8(9);
    b = bitget(a,1);
    c = bitget(a,2);
    d = bitget(a,3);
    e = bitget(a,4);
    disp(b);
    disp(c);
    disp(d);
    disp(e);
    f = uint16(10);
    g = bitget(f,1);
    h = bitget(f,2)
    i = bitget(f,3);
    j = bitget(f,4)
    disp(g);
    disp(h);
    disp(i);
    disp(j);        
   
    
endfunction
